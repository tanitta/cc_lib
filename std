tick=0.05000000001
local print_indent=function(indent)
	for i=1,indent do
		io.write("   ")
	end
end
table_print=function(table,indent)
	indent=indent or 0
	local flag=false
	if table[1]==nil then
		flag=true
	end
	print_indent(indent)
	print("{")
	for k,v in pairs(table) do
		if type(v)~="table" then
			print_indent(indent+1)
			if flag then
				io.write(k.."=")
			end
			if type(v)~="string" then
				print(tostring(v)..",")
			else
				print("\""..v.."\"")
			end
		else
			print_indent(indent)
			io.write(k.."=\n")
			table_print(v,indent+1)
		end
	end
	print_indent(indent)
	io.write("}")
	if indent~=0 then
		io.write(",")
	end
	io.write("\n")
end

print=function(...)
	local a={...}
	for i,v in ipairs(a) do
		if type(v)~="table" then
			io.write(tostring(v))
			if i~=#a then
				io.write(" ")
			end
		else
			if i~=1 then
				io.write("\n")
			end
			table_print(v)
		end
	end
	io.write("\n")
end
function split(str,pattern)
	local a={}
	local l=string.len(pattern)
	while(true)do
		local c=0
		for i=1,string.len(str) do
			if string.sub(str,i,i+l-1)==pattern then
				c=c+1
				table.insert(a,string.sub(str,1,i-1))
				str=string.sub(str,i+l)
				break
			end
		end
		if c==0 then
			break
		end
	end
	table.insert(a,str)
	return a
end
function matchCount(str,pattern)
	local c=0
	for v in string.gmatch (str, pattern) do
		c=c+1
	end
	return c
end
function max(...)
	local a=...
	local t=type(a)
	if t=="number" then
		return math.max(...)
	elseif t=="table" then
		local m=a[1]
		for i=2,#a do
			m=math.max(m,a[i])
		end
		return m
	end
end
function isContain(v,v1,v2)
	if v1>v2 then
		v1,v2=v2,v1
	end
	if v>=v1 and v<=v2 then
		return true
	end
	return false
end
