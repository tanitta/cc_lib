tick=0.05000000001
local print_indent=function(indent)
	for i=1,indent do
		io.write("   ")
	end
end
local table_print(table,indent)
	indent=indent or 0
	local flag=false
	if table[1]==nil then
		flag=true
	end
	print_indent(indent)
	print("{")
	for k,v in pairs(table) do
		if type(v)~="table" then
			print_indent(indent+1)
			if flag then
				io.write(k.."=")
			end
			print(tostring(v)..",")
		else
			table_print(v,indent+1)
		end
	end
	print_indent(indent)
	io.write("}")
	if indent~=0 then
		io.write(",")
	end
	io.write("\n")
end

print=function(...)
	local a={...}
	for i,v in ipairs(a) do
		if type(v)~="table" then
			io.write(tostring(v))
			if i~=#a then
				io.write(" ")
			end
		else
			table_print(v)
		end
	end
	io.write("\n")
end
multiTask=function(...)
	parallel.waitForAll(...)
end
function splitter(str,c)
	local a={}
	local l=string.len(c)
	for i=1,string.len(str) do
		if string.sub(str,i,i+l-1)==c then
			table.insert(a,string.sub(str,1,i-1))
			str=string.sub(str,i+l)
		end
	end
	table.insert(a,str)
	return a
end
function matchCount(str,pattern)
	local c=0
	for v in string.gmatch (str, pattern) do
		c=c+1
	end
	return c
end
function max(...)
	local a=...
	local t=type(a)
	if t=="number" then
		return math.max(...)
	elseif t=="table" then
		local m=a[1]
		for i=2,#a do
			m=math.max(m,a[i])
		end
		return m
	end
end
function isContain(v,v1,v2)
	if v1>v2 then
		v1,v2=v2,v1
	end
	if v>=v1 and v<=v2 then
		return true
	end
	return false
end
